<body>
{% block body %}

    <!-- Search model -->
    <div class="search-model">
        <div class="h-100 d-flex align-items-center justify-content-center">
            <div class="search-close-switch">+</div>
            <form class="search-model-form">
                <input type="text" id="search-input" placeholder="Search here.....">
            </form>
        </div>
    </div>
    <!-- Search model end -->

    <!-- Header Section Begin -->
    <header class="header-section">
        <div class="container-fluid">
            <div class="inner-header">
                <div class="logo">
                    <a href="{{ asset('./index.html') }}"><img src="{{ asset('client/img/logoo.png') }}" alt="" style="height:60px"></a>
                </div>
                <div class="header-right">
                    <img src="{{ asset('client/img/icons/search.png') }}" alt="" class="search-trigger">
                    <img src="{{ asset('client/img/icons/man.png') }}" alt="">
                    <a href="{{ asset('#') }}">
                        <img src="{{ asset('client/img/icons/bag.png') }}" alt="">
                        <span>2</span>
                    </a>
                </div>
                <div class="user-access">
                    <a href="{{ asset('#') }}">Register</a>
                    <a href="{{ asset('#') }}" class="in">Sign in</a>
                </div>
                <nav class="main-menu mobile-menu">
                    <ul>
                        <!-- class="active" -->
                        <li><a href="{{ asset('#') }}">Home</a></li>
                        <li><a href="{{ asset('#') }}">Shop</a>
                            <ul class="sub-menu">
                                <li><a href="{{ asset ('#') }}">Product</a></li>
                                <li><a href="{{ asset ('#') }}">Cart</a></li>
                                <li><a href="{{ asset ('#') }}">Check out</a></li>
                            </ul>
                        </li>
                        <li><a href="{{ asset ('#') }}">Forum</a></li>
                        <li><a href="{{ asset('#') }}">Event</a></li>
                        <li><a href="{{ asset('#') }}">Contact</a></li>
                    </ul>
                </nav>
            </div>
        </div>
    </header>
    <!-- Header Info Begin -->
    <div class="header-info">
        <div class="container-fluid">
            <div class="row">
                <div class="col-md-4">
                    <div class="header-item">
                        <img src="{{ asset('client/img/icons/delivery.png') }}" alt="">
                        <p>Free shipping on orders over 30DT in TUNISIA</p>
                    </div>
                </div>
                <div class="col-md-4 text-left text-lg-center">
                    <div class="header-item">
                        <img src="{{ asset('client/img/icons/voucher.png') }}" alt="">
                        <p>20% Student Discount</p>
                    </div>
                </div>
                <div class="col-md-4 text-left text-xl-right">
                    <div class="header-item">
                        <img src="{{ asset('client/img/icons/sales.png') }}" alt="">
                        <p>30% off on bikes. Use code: 30OFF</p>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- Header Info End -->
    <!-- Header End -->

    {% block main %}


    {% endblock %}

    <!-- Logo Section Begin -->
    <div class="logo-section spad">
        <div class="logo-items owl-carousel">
            <div class="logo-item">
                <img src="{{ asset('client/img/logos/logo-1.png') }}" alt="">
